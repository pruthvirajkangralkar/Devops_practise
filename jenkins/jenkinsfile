pipeline{
    agent any
    
    stages{
        stage('build'){
            steps{
                sh 'echo Build is now successful'
            }
        }
        stage ('deploy'){
            steps{
                sh 'echo deploy is sucessfull'
                }
        }    

	stage ('test'){
            steps{
                sh 'echo test now is sucessfull'
                }
        }

    }
}
